    "def suma(n):\n",
    "    suma = 0\n",
    "    n_operations = 0\n",
    "    for i in range (1,n):\n",
    "        suma = suma + i\n",
    "        n_operations = n_operations + 1\n",
    "    print(\"Numero de Operaciones: {}\" .format(n_operations))\n",
    "    return suma\n",
    "\n",
    "print(suma(5))"
   ]
